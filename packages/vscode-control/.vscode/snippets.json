{
	"Wallaby": {
		"prefix": "wal",
		"body": [
			"/*?*/"
		],
		"description": "wallaby log"
	},
	"Require": {
		"prefix": "req",
		"body": [
			"const { ${1:method} } = require('${0}')"
		],
		"description": "require"
	},
	"RequireX": {
		"prefix": "reqx",
		"body": [
			"const { ${1:method} } = require('./${1:method}')"
		],
		"description": "requireX"
	},
	"Test Snippet": {
		"prefix": "test",
		"body": [
      "test('happy', () => {",
      "\tconst result = $0",
      "\tconsole.log(result)",
			"})"
		],
		"description": "test"
	},
	"TestAsync": {
		"prefix": "testa",
		"body": [
			"test('happy', async () => {",
			"\tconst result = await $0",
			"\tconsole.log(result)",
			"})"
		],
		"description": "test async"
	},
	"commentMultiline": {
		"prefix": "com",
		"body": [
			"/**",
			" * $0",
			" */"
		],
		"description": "comment multiline"
	},
	"commentx": {
		"prefix": "comx",
		"body": [
			"// $0",
			"// ============================================"
		],
		"description": "comment with underline"
	},
	"StringTemplate": {
		"prefix": "str",
		"body": [
			"`${${0}}`"
		],
		"description": "str"
	},
	"StringTemplatex": {
		"prefix": "stx",
		"body": [
			"`${${0}}${}`"
		],
		"description": "stx"
	},
	"Export": {
		"prefix": "exportx",
		"body": [
			"exports.${1:module} = ${1:module}"
		],
		"description": "export"
	},
	"Expect Snippet": {
		"prefix": "exp",
		"body": [
			"expect(",
			"\t$0",
			").toEqual($1)"
		],
		"description": "ext"
	},
	"ExpectTruthy": {
		"prefix": "expt",
		"body": [
			"expect(",
			"\t$0",
			").toBeTruthy()"
		],
		"description": "ext"
	},
	"ExpectFalsy": {
		"prefix": "expf",
		"body": [
			"expect(",
			"\t$0",
			").toBeFalsy()"
		],
		"description": "x"
	},
	"ExpectSnapshot": {
		"prefix": "snap",
		"body": [
			"expect(",
			"\t$0",
			").toMatchSnapshot()"
		]
	},
	"ExpectInlineSnapshot": {
		"prefix": "inline",
		"body": [
			"expect(",
			"\t$0",
			").toMatchInlineSnapshot()"
		]
	},
	"Log as object": {
		"prefix": "lgr",
		"body": [
			"console.log({$0})"
		],
		"description": "log"
	},
	"Log Snippet": {
		"prefix": "lll",
		"body": [
			"console.log($0)"
		],
		"description": "simple log"
	},
	"Log Clipboard": {
		"prefix": "ccc",
		"body": [
			"console.log(`${CLIPBOARD}`, ${CLIPBOARD} $0)"
		],
		"description": "log clipboard"
	},
	"LogDebug": {
		"prefix": "lgd",
		"body": [
			"console.log({type: typeof ${1:toLog}})",
			"console.log({array: Array.isArray(${1:toLog}) ? ${1:toLog}.length : 'not array'})",
			"console.log( {keys: ${1:toLog} ? Object.keys(${1:toLog}) : 'not object'})",
			"console.log(${1:toLog})",
			"\n$0"
		],
		"description": "log and describe"
	}
}