{
	"name": "dep-fn",
	"version": "0.9.0",
	"description": "Easier update of Node.js dependencies",
	"main": "dist/index.js",
	"repository": "https://github.com/selfrefactor/services/tree/master/packages/dep-fn",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "cd src&&run lx",
		"out": "tsc -p .",
		"in": "tsc -p . --watch",
		"test": "jest"
	},
	"depFn": [
		"fs-extra"
	],
	"dependencies": {
		"fs-extra": "^7.0.0",
		"helpers-fn": "0.1.1",
		"init-puppeteer": "4.3.6",
		"inquirer": "^7.0.0",
		"json-format": "^1.0.1",
		"package-storage": "^1.0.0",
		"rambdax": "^3.7.0"
	},
	"devDependencies": {
		"@types/fs-extra": "8.1.0",
		"@types/jest": "25.1.4",
		"@types/node": "13.9.5",
		"@types/puppeteer": "1.20.2",
		"jest": "25.2.3",
		"ts-jest": "24.3.0",
		"tslint-fn": "^0.2.1",
		"typescript": "3.8.3"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/node_modules/helpers/jest/prepare.js",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": ".+spec.ts(x)?$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		]
	}
}
