{
	"name": "dep-fn",
	"version": "2.0.0",
	"description": "Easier update of Node.js dependencies",
	"main": "dist/index.js",
	"repository": "https://github.com/selfrefactor/services/tree/master/packages/dep-fn",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "cd src&&run lx",
		"out": "tsc -p .",
		"in": "tsc -p . --watch",
		"debug": "yarn out&&node dist/debug.js",
		"test": "jest"
	},
	"dependencies": {
		"fs-extra": "9.0.1",
		"helpers-fn": "1.3.2",
		"init-playwright": "0.25.3",
		"inquirer": "7.3.3",
		"json-format": "1.0.1",
		"package-storage": "1.0.0",
		"rambdax": "7.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "9.0.6",
		"@types/jest": "26.0.19",
		"@types/node": "14.14.20",
		"jest": "26.6.3",
		"ts-jest": "26.4.4",
		"tslint-fn": "0.2.2",
		"typescript": "4.1.3"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/node_modules/helpers-fn/jest/prepare.js",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": ".+spec.ts(x)?$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		]
	}
}
