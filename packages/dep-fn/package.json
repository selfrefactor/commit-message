{
	"name": "dep-fn",
	"version": "1.0.0",
	"description": "Easier update of Node.js dependencies",
	"main": "dist/index.js",
	"repository": "https://github.com/selfrefactor/services/tree/master/packages/dep-fn",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "cd src&&run lx",
		"out": "tsc -p .",
		"in": "tsc -p . --watch",
		"debug": "yarn out&&node dist/debug.js",
		"test": "jest"
	},
	"dependencies": {
		"fs-extra": "9.0.0",
		"helpers-fn": "0.2.0",
		"init-playwright": "0.1.0",
		"inquirer": "7.1.0",
		"json-format": "1.0.1",
		"package-storage": "1.0.0",
		"rambdax": "3.7.0"
	},
	"devDependencies": {
		"@types/fs-extra": "9.0.1",
		"@types/jest": "25.2.3",
		"@types/node": "14.0.4",
		"jest": "26.0.1",
		"ts-jest": "26.0.0",
		"tslint-fn": "0.2.2",
		"typescript": "4.0.0-dev.20200521"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/node_modules/helpers-fn/jest/prepare.js",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": ".+spec.ts(x)?$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		]
	},
	"commitMessage": {
		"workInProgress": ""
	}
}
