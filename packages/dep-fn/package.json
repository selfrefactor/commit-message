{
	"name": "dep-fn",
	"version": "0.8.2",
	"description": "Easier update of Yarn dependencies",
	"main": "dist/index.js",
	"repository": "https://github.com/selfrefactor/services/tree/master/packages/dep-fn",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "tslint --fix --project tsconfig.json",
		"prelint": "tsfmt -r --no-tsconfig src/*.ts",
		"out": "tsc -p .",
		"in": "tsc -p . --watch",
		"test": "jest"
	},
	"depFn": [
		"fs-extra"
	],
	"dependencies": {
		"fs-extra": "^7.0.0",
		"helpers": "https://github.com/selfrefactor/helpers#2.0.1",
		"init-puppeteer": "https://github.com/selfrefactor/init-puppeteer#8.0.0",
		"inquirer": "^7.0.0",
		"json-format": "^1.0.1",
		"package-storage": "^1.0.0",
		"rambdax": "^3.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/jest": "^24.0.22",
		"@types/node": "^12.12.6",
		"@types/puppeteer": "^1.20.2",
		"jest": "^25.0.0",
		"ts-jest": "^24.1.0",
		"tslint": "^6.0.0-beta1",
		"typescript": "^3.8.0-dev.20191105",
		"typescript-formatter": "^7.2.2"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/node_modules/helpers/jest/prepare.js",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": ".+spec.ts(x)?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json"
		]
	}
}